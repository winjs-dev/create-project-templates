{
  "name": "see-mirror-source",
  "version": "0.0.0",
  "buildVersion": "V202101-00-000",
  "private": true,
  "scripts": {
    "bootstrap": "yarn install || npm install || cnpm install",
    "format": "prettier --write \"*.{js,jsx,json,ts,tsx,css,less,scss,vue,html,md}\"",
    "deploy": "npm run build && npm run zip",
    "reinstall": "rimraf node_modules && rimraf yarn.lock && rimraf package.lock.json && npm run bootstrap",
    "escheck": "es-check",
    "zip": "node build/zip.js",
    "f2elint-scan": "f2elint scan --quiet --include src",
    "f2elint-fix": "f2elint fix --include src",
    "dev": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "node build/index.js --no-module",
    "lint": "vue-cli-service lint",
    "lint:style": "vue-cli-service lint:style",
    "report": "vue-cli-service build --report",
    "prepare": "husky install && npm run format",
    "gen:log": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint:ls-lint": "ls-lint",
    "lint:lint-staged": "lint-staged -c .husky/lint-staged.js",
    "build:see": "npm run build && deploy-cli see"
  },
  "dependencies": {
    "@winner-fed/cloud-utils": "^1.2.7",
    "@winner-fed/magicless": "*",
    "@winner-fed/win-ui": "^3.2.1",
    "amfe-flexible": "^2.2.1",
    "axios": "^1.2.1",
    "core-js": "^3.8.3",
    "normalize.css": "^8.0.1",
    "qs": "^6.10.3",
    "regenerator-runtime": "^0.14.1",
    "vue": "^2.7.13",
    "vue-router": "^3.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@ls-lint/ls-lint": "^1.9.2",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/runtime-dom": "^3.2.31",
    "@winner-fed/deploy-cli": "^0.0.5",
    "@winner-fed/f2elint": "^2.1.0",
    "@winner-fed/vue-cli-plugin-stylelint": "^1.0.2",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "archiver": "^3.0.0",
    "autoprefixer": "^10.4.12",
    "babel-plugin-import": "^1.13.3",
    "chalk": "^2.4.1",
    "compression-webpack-plugin": "^9.2.0",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "es-check": "^5.2.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^10.0.0",
    "husky": "^8",
    "less": "^4.1.2",
    "less-loader": "^8.0.0",
    "lint-staged": "^13",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.3.0",
    "rimraf": "^3.0.2",
    "svg-sprite-loader": "^6.0.11",
    "tasksfile": "^5.1.0",
    "unplugin-vue-components": "^0.22.7",
    "unplugin-vue2-script-setup": "^0.9.3",
    "webpack-manifest-plugin": "^4.1.1",
    "webpackbar": "^5.0.2"
  },
  "scripts-info": {
    "serve": "运行开发服务器",
    "build": "生产环境执行构建",
    "analyz": "生产环境执行构建打包分析",
    "deploy": "生产环境执行构建并压缩zip包",
    "build:see": "生成 see 平台部署发布物",
    "escheck": "检测是否含有 ES6+ 语法"
  }
}
