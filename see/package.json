{
  "name": "see",
  "version": "0.0.0",
  "buildVersion": "V202101-00-000",
  "scripts": {
    "bootstrap": "yarn --registry https://registry.npm.taobao.org || npm install --registry https://registry.npm.taobao.org || cnpm install",
    "format": "prettier --write \"src/**/*.{js,jsx,json,ts,tsx,css,less,scss,vue,html,md}\"",
    "deploy": "npm run build && npm run zip",
    "reinstall": "rimraf node_modules && rimraf yarn.lock && rimraf package.lock.json && npm run bootstrap",
    "escheck": "es-check",
    "zip": "node build/zip.js",
    "f2elint-scan": "f2elint scan",
    "f2elint-fix": "f2elint fix",
    "dev": "npm run serve",
    "serve": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5050",
    "report": "cross-env REPORT=true npm run build",
    "lint": "npm run f2elint-scan",
    "prepare": "npm run format && husky install",
    "gen:log": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint:ls-lint": "ls-lint",
    "lint:lint-staged": "./node_modules/.bin/nano-staged -c .husky/nano-staged.js",
    "build:see": "npm run build && node build/package/see.js"
  },
  "dependencies": {
    "@vue/composition-api": "^1.4.6",
    "@winner-fed/cloud-utils": "*",
    "@winner-fed/magicless": "*",
