{
  "name": "framework-v3-application-offline-buildtools-vite-uiframework-vant",
  "version": "0.0.0",
  "buildVersion": "V202101-00-000",
  "private": true,
  "scripts": {
    "bootstrap": "yarn install || npm install || cnpm install",
    "format": "prettier --write \"*.{js,jsx,json,ts,tsx,css,less,scss,vue,html,md}\"",
    "deploy": "npm run build && npm run zip",
    "reinstall": "rimraf node_modules && rimraf yarn.lock && rimraf package.lock.json && npm run bootstrap",
    "escheck": "es-check",
    "zip": "node build/zip.js",
    "f2elint-scan": "f2elint scan --quiet --include src",
    "f2elint-fix": "f2elint fix --include src",
    "serve:nomgw": "vue-cli-service serve --mode nomgw",
    "dev": "npm run serve",
    "serve": "vite",
    "build": "vite build",
    "build:no-cache": "npm run clean:cache && npm run build",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "preview": "vite preview --port 5050",
    "report": "cross-env REPORT=true npm run build",
    "lint": "npm run f2elint-scan --quiet",
    "prepare": "husky install && npm run format",
    "gen:log": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "lint:ls-lint": "ls-lint",
    "lint:lint-staged": "lint-staged -c .husky/lint-staged.js"
  },
  "dependencies": {
    "@winner-fed/cloud-utils": "^1.2.7",
    "@winner-fed/magicless": "*",
    "@winner-fed/mpaas-jsapi": "*",
    "amfe-flexible": "^2.2.1",
    "axios": "^1.2.1",
    "normalize.css": "^8.0.1",
    "qs": "^6.10.3",
    "vant": "^4.0.11",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@ls-lint/ls-lint": "^1.9.2",
    "@vitejs/plugin-legacy": "^3.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/runtime-dom": "^3.2.22",
    "@winner-fed/f2elint": "^2.1.0",
    "@winner-fed/unplugin-vue-setup-extend": "^0.0.3",
    "archiver": "^3.0.0",
    "autoprefixer": "^10.4.12",
    "chalk": "^2.4.1",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "es-check": "^5.2.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^10.0.0",
    "husky": "^8",
    "less": "^4.1.2",
    "lint-staged": "^13",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.3.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.7.1",
    "tasksfile": "^5.1.0",
    "unplugin-auto-import": "^0.6.1",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^4.3.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-imp": "^2.3.1",
    "vite-plugin-inspect": "^0.7.18",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-svg-loader": "^3.1.2"
  },
  "scripts-info": {
    "serve": "运行开发服务器",
    "build": "生产环境执行构建",
    "analyz": "生产环境执行构建打包分析",
    "deploy": "生产环境执行构建并压缩zip包",
    "build:see": "生成 see 平台部署发布物",
    "escheck": "检测是否含有 ES6+ 语法"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china",
    "rollup": "^2.56.3",
    "gifsicle": "5.2.0"
  },
  "engines": {
    "node": "^14.18 || >=16"
  }
}
